{
    "compilerOptions": {
      "baseUrl": "./",
      "downlevelIteration": true,
      "importHelpers": true,
      "lib": ["esnext"],
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strictNullChecks": true,
      "target": "es5",
      "outDir": "./dist"
    },
    "exclude": [
      "node_modules",
    ]
  }
  